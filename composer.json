{
  "name": "bitmotion/marketing-automation",
  "description": "Base TYPO3 extension that allows targeting and personalization of TYPO3 content: Limit pages, content elements etc. to certain \"Personas\". Determination of Personas can come from various sources (requires add-on extensions).",
  "type": "typo3-cms-extension",
  "homepage": "https://www.Leuchtfeuer.com",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "TYPO3 CMS",
    "Marketing",
    "Automation"
  ],
  "support": {
    "issues": "https://github.com/Leuchtfeuer/typo3-marketing-automation/issues",
    "source": "https://github.com/Leuchtfeuer/typo3-marketing-automation",
    "email": "dev@Leuchtfeuer.com"
  },
  "require": {
    "typo3/cms-backend": "^8.7 || ^9.5",
    "typo3/cms-core": "^8.7 || ^9.5",
    "typo3/cms-extbase": "^8.7 || ^9.5"
  },
  "autoload": {
    "psr-4": {
      "Bitmotion\\MarketingAutomation\\": "Classes/"
    }
  },
  "replace": {
    "bitmotion/marketing-automation": "self.version",
    "typo3-ter/marketing-automation": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "marketing_automation",
    }
  }
}
